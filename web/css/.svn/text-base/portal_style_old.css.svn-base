/* wrapper for entire portal. starts/ends after/before body tag */
#portal-container {
	/*width: 100%;*/

/*part of below IE hack to preserve min-width for portlet regions */
	padding: 0 350px 0 350px;
}

/* min width hack for IE */
#expander {
	margin: 0 -350px 0 -350px;
	position: relative;
  
}

